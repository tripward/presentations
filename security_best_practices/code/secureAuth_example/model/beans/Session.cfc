/**** @file model\beans\Session.cfc* @author Denard Springle ( denard.springle@gmail.com )* @description I am a session bean used for storing user sessions**/component displayname="Session" accessors="true" {	property userId;	property sessionId;	property hmacCode;	property firstName;	property lastName;	property role;	property lastActionAt;	/**	* @displayname init	* @description I am the constructor method for Session	* @return Type  this	*/	public function init( numeric userId = 0, string sessionId = '', string hmacCode = '', string firstName = '', string lastName = '', numeric role = 0, date lastActionAt = '2010-01-01 00:00:00' ) {		setUserId( arguments.userId );		setSessionId( arguments.sessionId );		setHmacCode( arguments.hmacCode );		setFirstName( arguments.firstName );		setLastName( arguments.lastName );		setRole( arguments.role );		setLastActionAt( arguments.lastActionAt );		return this;	}	// SETTERS //	public function setUserId( numeric userId = 0 ) {		variables.userId = arguments.userId;	}	public function setSessionId( string sessionId = '' ) {		variables.sessionId = arguments.sessionId;	}	public function setHmacCode( string hmacCode = '' ) {		variables.hmacCode = arguments.hmacCode;	}	public function setFirstName( string firstName = '' ) {		variables.firstName = arguments.firstName;	}	public function setLastName( string lastName = '' ) {		variables.lastName = arguments.lastName;	}	public function setRole( numeric role = 0 ) {		variables.role = arguments.role;	}	public function setLastActionAt( date lastActionAt = '2010-01-01 00:00:00' ) {		variables.lastActionAt = arguments.lastActionAt;	}	// GETTERS //	public function getUserId() {		return variables.userId;	}	public function getSessionId() {		return variables.sessionId;	}	public function getHmacCode() {		return variables.hmacCode;	}	public function getFirstName() {		return variables.firstName;	}	public function getLastName() {		return variables.lastName;	}	public function getRole() {		return variables.role;	}	public function getLastActionAt() {		return variables.lastActionAt;	}	// UTILITY //	public function getMemento() {		return variables;	}}